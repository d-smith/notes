  "source": "pragma solidity 0.8.10;\n\ncontract Faucet2 {\n\n    address owner;\n\n    constructor() {\n        owner = msg.sender;\n    }\n\n    function destroy() public {\n        require(msg.sender == owner);\n        selfdestruct(payable(owner));\n    }\n}",
